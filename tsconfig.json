{
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ],
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!node_modules/**"
  ],
  "compileOnSave": true,
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "admin/dynamic-navigation/admin.component.ts",
    "admin/dynamic-navigation/admin.ts",
    "admin/dynamic-navigation/boot.ts",
    "admin/dynamic-navigation/components/home/home.component.ts",
    "admin/dynamic-navigation/components/order/order.component.ts",
    "admin/dynamic-navigation/components/product/product.component.ts",
    "admin/dynamic-navigation/layout/navigator/navigator.ts",
    "admin/dynamic-navigation/services/router.service.ts",
    "admin/dynamic-routing/admin.component.ts",
    "admin/dynamic-routing/admin.ts",
    "admin/dynamic-routing/boot.ts",
    "admin/dynamic-routing/components/home/home.component.ts",
    "admin/dynamic-routing/components/order/order.component.ts",
    "admin/dynamic-routing/components/product/product.component.ts",
    "admin/dynamic-routing/layout/navigator/navigator.ts",
    "admin/dynamic-routing/services/router.service.ts",
    "app/boot.ts",
    "app/components/dynamic_navigation/dynamic-navigation.component.ts",
    "app/components/dynamic_routing/dynamic-routing.component.ts",
    "app/components/main.component.ts",
    "app/layout_components/footer/footer.ts",
    "app/layout_components/header/header.ts",
    "app/layout_components/navigator/navigator.ts",
    "app/layout_components/sidebar/sidebar.ts",
    "typings/browser.d.ts",
    "typings/browser/ambient/es6-shim/index.d.ts",
    "typings/browser/ambient/jasmine/index.d.ts"
  ]
}
